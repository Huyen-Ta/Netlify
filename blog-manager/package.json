{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "jest": {
    "verbose": true
  },
  "dependencies": {
    "apollo-boost": "^0.3.1",
    "apollo-cache-inmemory": "^1.4.3",
    "apollo-client": "^2.5.1",
    "apollo-link-http": "^1.5.16",
    "babel-preset-react-app": "^9.0.2",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "font-awesome": "^4.7.0",
    "gatsby": "^2.17.11",
    "gatsby-image": "^2.2.29",
    "gatsby-plugin-flow": "^1.1.10",
    "gatsby-plugin-manifest": "^2.2.23",
    "gatsby-plugin-offline": "^3.0.16",
    "gatsby-plugin-react-helmet": "^3.1.13",
    "gatsby-plugin-sass": "^2.1.21",
    "gatsby-plugin-sharp": "^2.2.31",
    "gatsby-plugin-typography": "^2.3.14",
    "gatsby-source-contentful": "^2.1.60",
    "gatsby-source-filesystem": "^2.1.33",
    "gatsby-source-graphql": "^2.1.20",
    "gatsby-theme-blog": "^1.1.16",
    "gatsby-transformer-sharp": "^2.2.23",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "lodash": "^4.17.13",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-apollo": "^2.4.1",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "sharp": "^0.30.5",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@storybook/addon-actions": "^5.2.6",
    "@storybook/addon-links": "^5.2.6",
    "@storybook/addons": "^5.2.6",
    "@storybook/react": "^5.2.6",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-gatsby": "^0.2.20",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-cypress": "^2.6.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "flow-bin": "^0.98.0",
    "flow-typed": "^2.5.1",
    "husky": "^2.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jsdom": "^12.1.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.11.0",
    "react-testing-library": "^6.0.2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "test": "jest --u --config config.jest.json",
    "test:coverage": "jest --coverage --u --config config.jest.json",
    "test:updateSnapshot": "jest --updateSnapshot --config config.jest.json",
    "pipeline": "yarn run eslint && yarn test:coverage",
    "lint": "eslint '**/*.{js,jsx}' --quiet",
    "flow": "flow",
    "prettier": "prettier --write  \"**/*.{js,jsx,json,md}\"",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn flow"
    }
  },
  "lint-staged": {
    "{src/**/*.js": [
      "prettier --write",
      "git add"
    ],
    "{src,tests,__mocks__,stories}/**/*.{js,jsx}": [
      "prettier --write",
      "eslint --cache --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
